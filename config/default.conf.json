{
    "Port": 8080,
    "MaxFileSize": 20971520,
    "HashLength": 7,
    "UserAgent": "ImgurGo (https://github.com/gophergala/ImgurGo)",
    "Stores" : [
        {
            "Type" : "s3",
            "BucketName" : "gophergala",
            "StoreRoot" : "",
            "Region" : "us-east-1",
            "NamePathRegex" : "([a-zA-Z0-9])([a-zA-Z0-9])([a-zA-Z0-9])([a-zA-Z0-9])[a-zA-Z0-9]{3}",
            "NamePathMap" : "${ImageSize}/${1}/${2}/${3}/${4}/${ImageName}"
        },
        {
            "Type" : "local",
            "StoreRoot": "/Users/jarvis/imagestore",
            "NamePathRegex" : "",
            "NamePathMap" : "${ImageName}/${ImageSize}"
        }
    ]
}
